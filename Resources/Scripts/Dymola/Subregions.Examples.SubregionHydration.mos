// Setup
removePlots();
createPlot(id = 1, filename = "SubregionHydration.mat");
species = cat(1,
              if 'inclC+' then {"graphite.'C+'"} else fill("",0),
              //if 'inclC19HF37O5S-' then {"ionomer.'C19HF37O5S-'"} else fill("",0),
              if 'incle-' then {"graphite.'e-'"} else fill("",0),
              if 'inclH+' then {"ionomer.'H+'"} else fill("",0),
              if inclH2 then {"gas.H2"} else fill("",0),
              if inclH2O then {"gas.H2O"} else fill("",0),
              if inclH2O then {"ionomer.H2O"} else fill("",0),
              if inclN2 then {"gas.N2"} else fill("",0),
              if inclO2 then {"gas.O2"} else fill("",0));

for i in 1:size(species, 1) loop
   // Define signals.
   y1 = {"subregion." + species[i] + ".rho"};
   y2 = {"subregion." + species[i] + ".tau_NE"};
   y3 = {"subregion." + species[i] + ".p_faces[1, 1]", "subregion." + species[i] + ".p",
         "subregion." + species[i] + ".p_faces[1, 2]"};
   y4 = {"subregion." + species[i] + ".faces[1, 1].T", "subregion." + species[i] + ".T",
         "subregion." + species[i] + ".faces[1, 2].T"};
   y5 = {"subregion." + species[i] + ".h",
         "subregion." + species[i] + ".chemical.mu"};
   y6 = {"subregion." + species[i] + ".physical.Ndot"};

   // Make plots.
   RunScript("FCSys/Resources/Scripts/Dymola/plot6.mos");
end for;
