        samplePeriod=samplePeriod,
        A=[-0.0893, 0, 0.0993; 0.4083, -2.0003, 0.0001; 0, 0, -2.0003],
        B=[-0.0001, -0.0791; -0.0002, 0.3622; 2.0003, 0],
        C=[1.7993, 13.216, 0; 0.8233, 0, 0])
