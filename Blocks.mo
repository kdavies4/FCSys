      connect(controller.act, observer.act) annotation (Line(
          points={{-41,-30},{-50,-30},{-50,6.10623e-16},{-11,6.10623e-16}},
          color={0,0,127},
          thickness=0.5,
          smooth=Smooth.None));
      connect(plant.u, controller.act) annotation (Line(
          points={{-41,30},{-50,30},{-50,-30},{-41,-30}},
